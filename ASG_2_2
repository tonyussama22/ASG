#include <stdio.h>
#include <math.h>


int main()
{
    int x, y, remainder, length = 0;
    int result = 0;

    printf("Enter an integer: ");
    printf("hi Github");
    printf("v2");
    scanf("%d", &x);

    y = x;

    while (y != 0)
    {
        y /= 10;
        ++length;
    }

    y = x;

    // calculate the result
    while (y != 0)
    {
        remainder = y % 10;
        result = result + pow(remainder, length);
        y /= 10;
    }

    // check if num is an Armstrong number
    if (result == x)
        printf("Armstrong number.\n");
    else
        printf("Not an Armstrong number.\n");

    return 0;
}
